{
    "name": "proto",
    "displayName": "Protobuf support",
    "description": "Protobuf support (.proto file syntax highlighting)",
    "version": "0.0.3",
    "publisher": "peterj",
    "license": "SEE LICENSE IN LICENSE.md",
    "icon": "images/protobuficon.png",
    "galleryBanner": {
        "color": "#5c2d91",
        "theme": "dark"
    },
    "homepage": "https://github.com/pj3677/vscode-protobuf/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/pj3677/vscode-protobuf/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pj3677/vscode-protobuf"
    },
    "engines": {
        "vscode": "^0.10.1"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "proto",
                "aliases": [
                    "Protocol Buffers",
                    "proto"
                ],
                "extensions": [
                    ".proto"
                ],
                "configuration": "./proto.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "proto",
                "scopeName": "source.proto",
                "path": "./syntaxes/proto.tmLanguage"
            }
        ]
    },
    "devDependencies": {
        "svgexport": "^0.4.2"
    },
    "scripts": {
        "vscode:prepublish": "svgexport ./images/protobuficon.svg ./images/protobuficon.png"
    }
}
